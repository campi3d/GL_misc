        float val = outBlack + (#Input[i] - InBlack) * grad;
        levelled[i] = pow(
                max( val, 0.0 )
                ,alpha );
    }
    if( $clamp)
    {
            // Clamp to 0-1 range
            levelled.rgb = clamp( levelled.rgb, vec3(0.0), vec3(1.0));
    }

