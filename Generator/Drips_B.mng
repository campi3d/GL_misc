    float inMid = 0.5;
    inMid = mix( InBlack, InWhite, inMid);

    float ratio = (inMid - InBlack) / (InWhite - InBlack);
    float alpha = 1.0 / pow(10.0, 1.0 - 2.0 * ratio);

    alpha = max(alpha, 0.0001);

    vec3 levelled = vec3(0.0);
    for (int i = 0; i != 3; ++i)
