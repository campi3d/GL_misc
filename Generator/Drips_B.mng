       <GLSL Type="MriSnBaseGlslContext">
        <objectName Type="QString">GLSL</objectName>
        <selected Type="bool">false</selected>
        <uuidString Type="QString">{58ec84bc-35e7-4647-8332-734e089d3840}</uuidString>
        <tags Type="QStringList"/>
